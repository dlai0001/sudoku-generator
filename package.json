{
  "name": "sudoku-generator",
  "version": "0.0.0",
  "description": "Sudoku generator for node",
  "author": {
    "name": "David Lai",
    "email": "me@dlai0001.com",
    "url": "dlai0001.com"
  },
  "main": "index.js",
  "repository": "https://github.com/dlai0001/sudoku-generator",
  "license": "MIT",
  "files": [
    "index.js",
    "sudoku-generator.js",
    "src/**/*.js"
  ],
  "bin": {
    "sudoku-generator": "sudoku-generator.js"
  },
  "keywords": [
    "sudoku-generator",
    "Sudoku"
  ],
  "dependencies": {
    "dependable": "git+https://github.com/westonganger/dependable.git",
    "lodash": "~3.6",
    "meow": "^3.1.0",
    "shuffle-array": "^0.1.0"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "grunt": "^1.0.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-mocha-cli": "^1.12.0",
    "grunt-mocha-cov": "^0.4.0",
    "jshint-stylish": "^1.0.1",
    "load-grunt-tasks": "^3.1.0",
    "mocha-lcov-reporter": "^0.0.2",
    "should": "^5.2.0",
    "time-grunt": "^1.1.0",
    "travis-cov": "^0.2.5"
  },
  "scripts": {
    "test": "grunt travis --verbose"
  },
  "config": {
    "blanket": {
      "spec-cov": {
        "threshold": 80,
        "localThreshold": 80
      },
      "pattern": [
        "src"
      ],
      "data-cover-never": "test"
    },
    "travis-cov": {
      "threshold": 80
    }
  }
}
